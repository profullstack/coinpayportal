#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ettinger/src/profullstack.com/coinpayportal/node_modules/.pnpm/vitest@2.1.9_@types+node@22.7.5_@vitest+ui@1.6.1_vitest@1.6.1__jsdom@23.2.0_bufferutil@_fd1a0b88891612bf6865ae3cbd2b32f1/node_modules/vitest/node_modules:/home/ettinger/src/profullstack.com/coinpayportal/node_modules/.pnpm/vitest@2.1.9_@types+node@22.7.5_@vitest+ui@1.6.1_vitest@1.6.1__jsdom@23.2.0_bufferutil@_fd1a0b88891612bf6865ae3cbd2b32f1/node_modules:/home/ettinger/src/profullstack.com/coinpayportal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ettinger/src/profullstack.com/coinpayportal/node_modules/.pnpm/vitest@2.1.9_@types+node@22.7.5_@vitest+ui@1.6.1_vitest@1.6.1__jsdom@23.2.0_bufferutil@_fd1a0b88891612bf6865ae3cbd2b32f1/node_modules/vitest/node_modules:/home/ettinger/src/profullstack.com/coinpayportal/node_modules/.pnpm/vitest@2.1.9_@types+node@22.7.5_@vitest+ui@1.6.1_vitest@1.6.1__jsdom@23.2.0_bufferutil@_fd1a0b88891612bf6865ae3cbd2b32f1/node_modules:/home/ettinger/src/profullstack.com/coinpayportal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
